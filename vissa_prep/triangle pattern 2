#include <stdio.h>

int main() {
    int N;
    
    // Read the height of the triangle
    scanf("%d", &N);

    // Loop through each row
    for (int i = 1; i <= N; i++) {
        // Print the number 'i', 'i' times
        for (int j = 1; j <= i; j++) {
            if (j != 1) {
                printf(" ");
            }
            printf("%d", i);
        }
        // Move to the next line after each row
        printf("\n");
    }

    return 0;
}

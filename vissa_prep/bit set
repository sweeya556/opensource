#include <stdio.h>

int main() {
    // Declare the input variables
    long long N;
    int k;

    // Read the input
    scanf("%lld %d", &N, &k);

    // Check if the k-th bit is set
    if (N & (1LL << (k - 1))) {
        printf("true\n");
    } else {
        printf("false\n");
    }

    return 0;
}
